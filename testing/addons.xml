<addons>
    <addon id="plugin.video.tvtime.private" name="TV Time" provider-name="redacted" version="2.5.2">
        <requires>
        	<import addon="xbmc.python" version="2.1.0" />
        	<import addon="script.module.requests" version="2.12.4" />
        	<import addon="script.module.tvtime.common" version="1.0.0"/>
        	<import addon="plugin.video.youtube" version="5.5.1"/>
        	<import addon="script.module.m7lib" version="0.1.0" />
        	<import addon="script.module.urlresolver" version="3.0.0" />
        </requires>
        <extension library="main.py" point="xbmc.python.pluginsource">
            <provides>video</provides>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Watch Major Television Networks Live for Free!</summary>
            <description>Watch a ton of live TV from various sources as well as movies on demand!</description>
            <platform>all</platform>
        </extension>
    </addon>

    <addon id="script.module.tvtime.common" name="TV Time Common Libraries" version="1.0.0" provider-name="redacted">
        <requires>
            <import addon="xbmc.python" version="2.1.0"/>
        </requires>
        <extension point="xbmc.python.module" library="lib"/>
        <extension point="xbmc.addon.metadata">
            <platform>all</platform>
            <summary lang="en">Collection of modules used by TV Time</summary>
            <description lang="en">Collection of modules used by TV Time</description>
            <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
        </extension>
    </addon>

    <addon id="repository.redacted" name="redacted Private Add-on Repository" version="1.0.4" provider-name="redacted">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="redacted Private Add-on Repository">
            <info compressed="false">https://github.com/kmorgz/skydiving-lessons/raw/master/testing/addons.xml</info>
            <checksum>https://github.com/kmorgz/skydiving-lessons/raw/master/testing/addons.xml.md5</checksum>
            <datadir zip="true">https://github.com/kmorgz/skydiving-lessons/raw/master/testing/</datadir>
            <hashes>false</hashes>
            <dir>
                <info compressed="false">https://github.com/tvaddonsco/tva-resolvers-repo/raw/master/addons.xml</info>
                <checksum>https://github.com/tvaddonsco/tva-resolvers-repo/raw/master/addons.xml.md5</checksum>
                <datadir zip="true">https://github.com/tvaddonsco/tva-resolvers-repo/raw/master/zips/</datadir>
